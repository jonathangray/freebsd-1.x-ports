test -n "$silent" || echo "checking for lgamma"
cat > conftest.${ac_ext} <<EOF
#include "confdefs.h"
#include <ctype.h>
int main() { return 0; }
int t() { 
/* The GNU C library defines this for functions which it implements
    to always fail with ENOSYS.  Some functions are actually named
    something starting with __ and the normal name is an alias.  */
#if defined (__stub_lgamma) || defined (__stub___lgamma)
choke me
#else
/* Override any gcc2 internal prototype to avoid an error.  */
extern char lgamma(); lgamma();
#endif
; return 0; }
EOF
if eval $ac_compile; then
  rm -rf conftest*
  {
test -n "$verbose" && \
echo "	defining" GAMMA to be "lgamma"
echo "#define" GAMMA "lgamma" >> confdefs.h
DEFS="$DEFS -DGAMMA=lgamma"
}


else
  rm -rf conftest*
  {
test -n "$verbose" && \
echo "	defining" GAMMA to be "gamma"
echo "#define" GAMMA "gamma" >> confdefs.h
DEFS="$DEFS -DGAMMA=gamma"
}

fi
